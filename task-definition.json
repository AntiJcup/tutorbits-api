{
    "containerDefinitions": [
        {
            "name": "tutorbits-api-container",
            "image": "",
            "cpu": 1024,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [],
            "environment": [
                {
                    "name": "ASPNETCORE_ENVIRONMENT",
                    "value": "AWSStaging"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "COGNITO_USER_POOL_CLIENT_SECRET",
                    "valueFrom": "alias/aws/ssm"
                },
                {
                    "name": "GOOGLE_OATH_KEY",
                    "valueFrom": "alias/aws/ssm"
                },
                {
                    "name": "SQL_PWD",
                    "valueFrom": "alias/aws/ssm"
                },
                {
                    "name": "SQL_UID",
                    "valueFrom": "alias/aws/ssm"
                }
            ],
            "startTimeout": 30,
            "stopTimeout": 10,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/tutorbits-api-task-definition",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "tutorbits-api-task-definition",
    "taskRoleArn": "arn:aws:iam::518121476465:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::518121476465:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "4096"
}